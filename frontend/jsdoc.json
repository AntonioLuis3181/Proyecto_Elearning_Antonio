{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.(js|jsx)$",
    "excludePattern": "(node_modules/|docs/)"
  },
  "plugins": ["plugins/markdown", "node_modules/jsdoc-babel"],
  "babel": {
    "presets": ["@babel/preset-react"]
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "README.md"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}